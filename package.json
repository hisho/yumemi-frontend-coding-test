{
  "name": "yumemi-frontend-coding-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev -p 3000",
    "dev:generate-env": "generate-env-declare --watch",
    "dev:tcm": "tcm -p 'src/**/*.module.css' . -w --camelCase",
    "build": "run-s build:tcm build:next",
    "build:next": "next build",
    "build:tcm": "tcm -p 'src/**/*.module.css' . --camelCase",
    "start": "next start",
    "lint": "run-p lint:*",
    "lint:next": "next lint --dir src --ignore-path .prettierignore --max-warnings 0",
    "lint:prettier": "prettier --check .",
    "lint:typecheck": "tsc --pretty --noEmit",
    "lint:stylelint": "stylelint --ignore-path .gitignore './src/**/*.css'",
    "format": "run-s format:eslint format:prettier format:stylelint",
    "format:eslint": "yarn lint:next --fix",
    "format:prettier": "yarn lint:prettier --write",
    "format:stylelint": "stylelint --ignore-path .gitignore './src/**/*.css' --fix",
    "test": "run-s test:*",
    "test:api": "playwright test src/pages/api -c playwright.config.api.ts",
    "test:component": "playwright test src -c playwright.config.component.ts"
  },
  "dependencies": {
    "highcharts": "^10.3.2",
    "highcharts-react-official": "^3.1.0",
    "next": "13.0.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typesafe-utils": "^1.16.2",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@hisho/generate-env-declare": "^0.0.3",
    "@next/font": "13.0.7",
    "@playwright/experimental-ct-react": "^1.29.0",
    "@playwright/test": "^1.29.0",
    "@tsconfig/strictest": "^1.0.2",
    "@types/eslint": "8.4.10",
    "@types/node": "18.11.16",
    "@types/prettier": "^2.7.1",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "autoprefixer": "^10.4.13",
    "dotenv": "^16.0.3",
    "eslint": "8.30.0",
    "eslint-config-next": "13.0.7",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "postcss-custom-media": "^9.0.1",
    "prettier": "^2.8.1",
    "stylelint": "^14.16.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^29.0.0",
    "typed-css-modules": "^0.7.2",
    "typescript": "4.9.4"
  },
  "volta": {
    "node": "18.12.1",
    "yarn": "1.22.19"
  }
}
