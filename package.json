{
  "name": "yumemi-frontend-coding-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev -p 3000",
    "dev:generate-env": "generate-env-declare --watch",
    "build": "run-s build:generate-env build:next",
    "build:next": "next build",
    "build:generate-env": "generate-env-declare",
    "start": "next start",
    "lint": "run-p lint:*",
    "lint:next": "next lint --dir src --ignore-path .prettierignore --max-warnings 0",
    "lint:prettier": "prettier --check .",
    "lint:typecheck": "tsc --pretty --noEmit",
    "format": "run-s format:eslint format:prettier",
    "format:eslint": "yarn lint:next --fix",
    "format:prettier": "yarn lint:prettier --write"
  },
  "dependencies": {
    "next": "13.0.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@hisho/generate-env-declare": "^0.0.3",
    "@next/font": "13.0.7",
    "@tsconfig/strictest": "^1.0.2",
    "@types/eslint": "8.4.10",
    "@types/node": "18.11.16",
    "@types/prettier": "^2.7.1",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "8.30.0",
    "eslint-config-next": "13.0.7",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "typescript": "4.9.4"
  },
  "volta": {
    "node": "18.12.1",
    "yarn": "1.22.19"
  }
}
